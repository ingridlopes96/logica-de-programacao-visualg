algoritmo "divisão"
// Função :
// Autor :
// Data : 13/01/2025
// Seção de Declarações 
var
          resultado, num1, num2 : real
inicio
// Seção de Comandos

Escreva ("Digite o primeiro número: ")
Leia(num1)

Escreva ("Digite o segundo número: ")
Leia (num2)

resultado <- num1 / num2

Escreva ("O resultado da divisao é: ", resultado)

fimalgoritmo
