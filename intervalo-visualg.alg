algoritmo "intervalo"

var
   numero: inteiro
inicio
      Escreval ("Digite um número")
      Leia (numero)
      
      Se (numero >=0) e (numero <=150) entao
         Escreval ("O número está dentro do intervalo")
      senao
           Escreval ("O número está fora do intervalo")
      Fimse
fimalgoritmo
